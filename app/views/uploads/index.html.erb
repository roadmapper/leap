<h1>Upload</h1>

		<%= form_tag({:action => :upload, :controller=>"dashboard"}, :multipart => true) do %>
		  <%= file_field_tag 'file' %>
		<br>
		<%= submit_tag( "Upload", class: 'btn btn-primary') %>
		<br>
		<% end %>
		

		<div id="ajax">
		<!--% render :partial => "report" %-->
		</div>


	<h1>Uploads</h1>
<table class="table table-striped">
  <tr>
    <th>File Name</th>
    <th>Status</th>
    <th>Upload Date</th>
    <th>Process Date</th>
    <th>Uploaded by</th>
    <th></th>
    <th></th>
  </tr>

<% @uploads.each do |upload| %>
  <tr>
    <td><%= upload.file_name %></td>
    <td><%= upload.status %></td>
    <td><%= upload.upload_date %></td>
    <td><%= upload.process_date %></td>
    <td><%= upload.uploaded_by %></td>
    <td><%#link_to 'Process', process_uploads_path(upload, method: :get), data: { confirm: 'Are you sure?' } %></td>
    <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
