<%= render "layouts/navigation" %>
<div class="container">
	<h1>Missing Account Info</h1>
	<%= link_to 'Back', root_path, class:  "btn btn-primary btn-large" %>
</div>
<hr class="featurette-divider">
<div class="container">
	<table class="table-responsive table-bordered">
	  <tr>
	    <th>Owner Name</th>
	    <th>Customer Unique ID</th>
	    <th>Company Name</th>
	    <th>Account Number</th>
	  </tr>

	<% @records_array.each do |record| %>
	  <tr>
	    <td><%= record[0] %></td>
	    <td><%= record[1] %></td>
	    <td><%= record[2] %></td>
	    <td><%= record[3] %></td>
	  </tr>
	<% end %>
	</table>

	<br />
</div>

