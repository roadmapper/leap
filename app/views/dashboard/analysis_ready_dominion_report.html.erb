<h1>Ready to Analyze Customers for Dominion Virginia Power</h1>

<table class="table table-striped" border="1">
  <tr>
    <th>Owner Name</th>
    <th>Account Number</th>
    <th>Meter Readings</th>
    <th>Ready to Analyze</th>
  </tr>

<% @records_array.each do |record| %>
  <tr>
    <td><%= link_to record[0], url_for(:controller => "dashboard", :action => "property_report", :owner => record[0]) %></td>
    <td><%= record[1] %></td>
    <td><%= record[2] %></td>
    <td><%= record[3] %></td>
  </tr>
<% end %>
</table>

<br />
<%= link_to 'Back', dashboard_index_path %>
