<h1>Editing User</h1>
<%= form_for(@user) do |f| %>
	<%= f.label :email %><br>
      	<%= f.text_field :email %>
	<br>
      	<%= f.label :password %><br>
      	<%= f.password_field :password %>
	<br>
	<%= f.label :password_confirmation, "Confirmation" %><br>
      	<%= f.password_field :password_confirmation %>
	<br><br>
      	<%= f.submit "Update User", class: "btn btn-large btn-primary" %>
<% end %>
